modules = ["nodejs-20", "python-3.11"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Educational File Server"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "display_alpha_file"

[[workflows.workflow]]
name = "Educational File Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python3 -m http.server 5000"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow]]
name = "display_alpha_file"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cat alpha.txt"

[[ports]]
localPort = 5000
externalPort = 80
